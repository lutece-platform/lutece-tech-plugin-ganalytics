<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>ganalytics</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>0.0.4</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>ganalytics.plugin.description</description>
    <provider>ganalytics.plugin.provider</provider>
    <provider-url>http://your.web.site.com</provider-url>
    <icon-url>images/admin/skin/plugins/ganalytics/folder_home.PNG</icon-url>
    <copyright/>
    <db-pool-required>1</db-pool-required>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>GANALYTICS_MANAGEMENT</feature-id>
            <feature-title>ganalytics.adminFeature.ganalytics_management.name</feature-title>
            <feature-description>ganalytics.adminFeature.ganalytics_management.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/ganalytics/getManageGanalyticsCode.jsp</feature-url>
            <feature-icon-url/>
            <feature-group> CONTENT </feature-group>
      </admin-feature>
    </admin-features>
    
    <!-- Page Include Service -->
    <page-include-services>
        <page-include-service>
            <page-include-service-id>ganalytics.include.service.name</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.ganalytics.web.GanalyticsInclude</page-include-service-class>
        </page-include-service>
    </page-include-services>


</plug-in>